<!--
  - sdm-archive.xml
  -
  - Version:  $Revision$
  -
  - Date:     $Date$
  -
  - State Darwin Museum Archive schema
  - 
  - <dc-type>
  -   <schema></schema>
  -   <element></element>
  -   
  -   <scope_note></scope_note>
  - </dc-type>
  -->

<!-- start of XML -->

<dspace-dc-types>

  <dspace-header>
    <title>SDM Archive types Registry</title>
    <contributor.author>Petya Kohts</contributor.author>
    <contributor.author>Irina Kalacheva</contributor.author>
    <contributor.editor>$Author$</contributor.editor>
    <date.created>$Date$</date.created>
    <description>$Log$</description>
    <description.version>$Revision$</description.version>
  </dspace-header>

  <dc-schema>
      <name>sdm-archive</name>
      <namespace>http://kohts.com/sdm/archive/</namespace>
  </dc-schema>

  <dc-type>
    <schema>sdm-archive</schema>
    <element>date</element>
    <qualifier>cataloged</qualifier>
    <scope_note>populated with the date when the item changes its state to "In catalog" (equals to dc.date.created in most cases)</scope_note>
  </dc-type>

  <dc-type>
    <schema>sdm-archive</schema>
    <element>date</element>
    <qualifier>digitized</qualifier>
    <scope_note>populated with the date when the item changes its state to "Digitized" (it was scanned, photoed or digitally recorded in any other way and those recordings were attached to the item in dspace as a separate bitstream)</scope_note>
  </dc-type>

  <dc-type>
    <schema>sdm-archive</schema>
    <element>date</element>
    <qualifier>textExtracted</qualifier>
    <scope_note>populated with the date when item was complemented with application/pdf bitstream or text/html bitstream (this happens as a result of optical character recognition process)</scope_note>
  </dc-type>

  <dc-type>
    <schema>sdm-archive</schema>
    <element>date</element>
    <qualifier>scanScheduled</qualifier>
    <scope_note>Date(s) when scan was scheduled for the item</scope_note>
  </dc-type>

  <dc-type>
    <schema>sdm-archive</schema>
    <element>misc</element>
    <qualifier>inventoryGroup</qualifier>
    <scope_note>There were two inventories of Alexander Erich Kohts archives: by Novikova (1998) and by Kalacheva (2010-2014)</scope_note>
  </dc-type>
  <dc-type>
    <schema>sdm-archive</schema>
    <element>misc</element>
    <qualifier>storageItem</qualifier>
    <scope_note></scope_note>
  </dc-type>
  <dc-type>
    <schema>sdm-archive</schema>
    <element>misc</element>
    <qualifier>fond</qualifier>
    <scope_note></scope_note>
  </dc-type>

  <dc-type>
    <schema>sdm-archive</schema>
    <element>misc</element>
    <qualifier>completeness</qualifier>
    <scope_note></scope_note>
  </dc-type>
  <dc-type>
    <schema>sdm-archive</schema>
    <element>misc</element>
    <qualifier>authenticity</qualifier>
    <scope_note></scope_note>
  </dc-type>
  <dc-type>
    <schema>sdm-archive</schema>
    <element>misc</element>
    <qualifier>document-type</qualifier>
    <scope_note></scope_note>
  </dc-type>
  <dc-type>
    <schema>sdm-archive</schema>
    <element>misc</element>
    <qualifier>classification-code</qualifier>
    <scope_note></scope_note>
  </dc-type>
  <dc-type>
    <schema>sdm-archive</schema>
    <element>misc</element>
    <qualifier>classification-group</qualifier>
    <scope_note></scope_note>
  </dc-type>
  <dc-type>
    <schema>sdm-archive</schema>
    <element>misc</element>
    <qualifier>archive-date</qualifier>
    <scope_note></scope_note>
  </dc-type>
  <dc-type>
    <schema>sdm-archive</schema>
    <element>misc</element>
    <qualifier>notes</qualifier>
    <scope_note></scope_note>
  </dc-type>

</dspace-dc-types>
